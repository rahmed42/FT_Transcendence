{
	"name": "frontend",
	"description": "Frontend for FT_transcendance project",
	"version": "1.0.0",
	"scripts": {
		"startgame": "parcel serve src/app.html",
		"buildgame": "parcel build index.html",
		"testgame": "echo \"Error: no test specified\" && exit 1",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"keywords": [
		"colyseus",
		"phaser"
	],
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@parcel/packager-raw-url": "^2.8.3",
		"@parcel/transformer-webmanifest": "^2.8.3",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.26.0",
		"parcel": "^2.4.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"process": "^0.11.10",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"axios": "^1.4.0",
		"colyseus.js": "^0.15.0-preview.3",
		"phaser": "^3.60.0",
		"stable": "^0.1.8",
		"svelte-particles": "^2.9.3",
		"tsparticles": "^2.9.3"
	}
}
