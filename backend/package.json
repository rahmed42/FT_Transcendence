{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for FT_transcendance project",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "startgame": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@colyseus/arena": "^0.15.0-preview.1",
    "@colyseus/monitor": "^0.15.0-preview.1",
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/platform-socket.io": "^9.4.2",
    "@nestjs/websockets": "^9.4.2",
    "@prisma/client": "^4.14.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^9.0.1",
    "argon2": "^0.30.3",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "colyseus": "^0.15.0-preview.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.16.4",
    "formidable": "^2.1.1",
    "jsonwebtoken": "^9.0.0",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "qrcode": "^1.5.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "socket.io-client": "^4.6.1",
    "superagent": "^8.0.9",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@colyseus/loadtest": "^0.15.0-preview.0",
    "@colyseus/testing": "^0.15.0-preview.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/mocha": "^8.2.3",
    "@types/node": "18.15.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/qrcode": "^1.5.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "mocha": "^9.0.2",
    "prettier": "^2.3.2",
    "prisma": "^4.14.1",
    "rimraf": "^2.7.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
